<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.saccbackend3.mapper.StudentMapper">

    <insert id="addStudent" parameterType="com.example.saccbackend3.entity.Student">
        insert into saccbackend3.student(id,name,sex,age,email)values (#{id},#{name},#{sex},#{age},#{email})
    </insert>

    <delete id="deleteStudentById" parameterType="java.lang.String">
        delete from saccbackend3.student where id = #{id}
    </delete>

    <update id="updateStudent" parameterType="com.example.saccbackend3.entity.Student">
        update saccbackend3.student
        set name = #{name}, sex = #{sex}, age = #{age}, email = #{email}
        where id = #{id};
    </update>

    <select id="listStudent" resultType="com.example.saccbackend3.entity.Student">
        select *
        from saccbackend3.student
        order by id
    </select>

    <select id="listStudentByAges" parameterType="java.lang.Integer" resultType="com.example.saccbackend3.entity.Student">
        select *
        from saccbackend3.student
        where age &gt;= #{arg0} and age &lt;= #{arg1}
        order by id
    </select>
</mapper>